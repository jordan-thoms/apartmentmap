<script type="text/javascript">
    function limitText(elementId, fieldToLimit, buttonId) {
        var $textArea = elementId;
        var $FeedbackTextArea = $("#" + fieldToLimit);
        var submitButton = $("#" + buttonId);
        var mylength = $textArea.value.length;
        var countElement = $("#" + fieldToLimit + "Count");

        submitButton.disabled = false;

        if (mylength <= 0) {
            submitButton.disabled = true;
        } else if (mylength > 500) {

            $FeedbackTextArea.val($FeedbackTextArea.val().substring(0, 500));

            $textArea.selectionStart = $textArea.value.length;
            $textArea.selectionEnd = $textArea.value.length;
            $textArea.focus();
        }

        countElement.html(500 - $textArea.value.length);

        return true;

    }

    function validateSellerEmail() {
        var emailBox = jQuery(".ContactAgentEmailSeller");
        if (emailBox.length > 0) {

            var text = emailBox.val();

            // remove all symbols, leave only normal characters.
            text = text.replace(/[^A-Za-z]/g, "");
            if (text.length == 0) {
                jQuery("#EmailErrorMessage").text("Please enter your enquiry below");
                jQuery("#ClassifiedActions_EmailForm").css("background-color", "#ff0");
                return false;
            } else {
                return true;
            }

        } else {
            return false;
        }
    }
</script>

