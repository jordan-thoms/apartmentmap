            if(false) {
                
                //tracking links for motors
                _V_("myVideo").addEvent("play", function () {
                    //establish if we are starting the video or resuming
                    if(userHasStartedVideo) {
                        //this is a resume
                        TrackVideoAction(57316);
                    }
                    else {
                        TrackVideoAction(57314);
                        PingAutobase();
                        userHasStartedVideo = true;
                    }
                });
                _V_("myVideo").addEvent("pause", function () { TrackVideoAction(57315); });
                _V_("myVideo").addEvent("ended", function () {
                    TrackVideoAction(57320);
                    userHasStartedVideo = false;
                });
                _V_("myVideo").addEvent("ended", function () { _V_("myVideo").loadingSpinner.hide(); });       
                _V_("myVideo").addEvent("volumechange", function () { VideoPlayerVolumeChange(57318, 57319); });
                _V_("myVideo").addEvent("error", function () {
                    _V_("myVideo").pause();
                    //remove the video player
                    $("#myVideo").html("<p style=\"color: white;\">We’re sorry, this video is currently unavailable</p>");
                });
                 videoExitTrackingId = 57317;
            }    
            else {
                //tracking links for other sections 
                 _V_("myVideo").addEvent("play", function () { TrackVideoAction(54421); });
                _V_("myVideo").addEvent("pause", function () { TrackVideoAction(54422); });
                _V_("myVideo").addEvent("ended", function () { TrackVideoAction(54423); });
            }
            
        }
        
        function PingAutobase(autobaseId) {
                      //need to tell Autobase that someone has pressed play
